{Entity
	(include "/properties/tank.ext")
      (include "/properties/structure/tank.ext")
	(include "/properties/shadow/tank.inc" scale(0.9))
	(include "/properties/selection/vehicle.inc" scale(1.2))
	{props "light" "average"}
	
	{Volume "Turret"
		{thickness  38
			{top		13}
			{front	51}
			{rear	 	38 0.3}
			{left		38 0.3}
			{right	38 0.3}
		}
	}
	{Volume "Gun"
		{thickness 10}
	}
	{Volume "Body"
		{thickness  38
			{top		13 1.0}
			{bottom	13 1.0}
			{front	67}
			{rear	 	25 0.6}
			{left		25 0.6}
			{right	25 0.6}
		}
	}

	("crew_4_human")
	{boarder
		{anm "left"
			{forward	{begin	"board_tank_left"}}
			{reverse 	{end	"emit_tank_1_hold"}}
		}
		{anm "right"
			{forward	{begin	"board_tank_right"}}
			{reverse 	{end	"emit_tank_1_hold"}}
		}
      }
	{Weaponry
		{place "gun"
			{DefaultWeapon "37mm_m6"}
			{Mounted}
			{weapon {ammo "ammo bullet37 ap" 1}}
		}
		{place "mgun"
			{LinkBone "mgun"}
			{DefaultWeapon "browning"}
			{Mounted}
			{weapon {ammo "ammo mgun" 200}}
		}
      }
	{extender "inventory"
		{weapon "gun" {fixed}}
		{box
			{item "ammo mgun" 900}
                  {item "ammo bullet37 ap" 56}
                  {item "ammo bullet37 fg" 42}
		}
	}
	
	{mass 14000}
	{Chassis
		{track_size 0.6 0.7}
		{Locomotion 
			{MaxSpeed 	    26} ; максимальная скорость, kmph
			{StartTime 		10} ; время разгона до макс. скорости с 0, seconds
			{BrakeTime 		2} ; время торможения с макс. скорости до 0, seconds
			{StopTime		 1} ; время остановки после отключения двигателя с макс. скорости до 0, seconds
			{TurnRadius     5} ; мин. радиус разворота, meter
			{TurnTime		5} ; время на разворот на 360 градусов при макс. скорости разворота, seconds
			{TurnStart		 2}	; время разгона до макс. угловой скорости с 0, seconds
			{TurnStop	   0.5}	; время остановки с макс. угловой скорости до 0, seconds
			{Gears 0.2 0.3 0.5 0.7 1.0} ; передаточные коэф-ты коробки передач
			{RearGears 0.2 0.5} ; передаточные коэф-ты коробки передач
		}
	}
	{Extension "m5a1.mdl"}

(mod "mp"
		{sensor
			{Visor "main"
				{vision "Visor_Tank"}
			}
		}
            {Chassis
			{speed
				{Slow	11	}	; km/h
				{Normal	16	}
				{Fast	26	}
			}
   {FuelBag
          {volume 289 }	; liters
	    {rate   187 }   	; liters per kilometer
	    {remain 289 }
          {fuel "fuel"}
        }

			{Locomotion ("locomotion")
				{MaxSpeed 26}
			}
		}

	)

}