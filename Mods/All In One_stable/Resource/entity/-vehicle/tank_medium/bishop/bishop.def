			{weapon {ammo "ammo mgun" 30}}
		}
	}
	{extender "inventory"
		{weapon "gun" {fixed}}
            {weapon "mgun" {fixed}}
		{box
			{item "ammo mgun" 300}
			{item "ammo bullet95 fg" 31}
		}
	}
	
	{mass 17500}
	{Chassis
		{track_size 0.6 0.7}
