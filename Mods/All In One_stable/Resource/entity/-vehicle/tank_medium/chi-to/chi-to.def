{Entity
	(include "/properties/tank.ext")
	(include "/properties/shadow/tank.inc"  scale(1.2))
	(include "/properties/selection/vehicle.inc"  scale(1.2))
	{props "medium" "high" "turretmg2"}
	{TexMods
		{"ambush" "1"}
	}
	{volume "body"
		{tags "body"}
		{component "body"}
		{thickness 50
		    {front  75 0.2}
		    {rear   20 0.2}
                        {top 15}
			{bottom	10}
		}   
	}
	{volume "body1"
		{tags "body"}
		{component "body"}
		{thickness 50
		    {front  75 0.2}
		    {rear   20 0.2}
                        {top 15}
			{bottom	10}
		}
	}
	{volume "turret"
		{thickness 50
			{top    10}
			{front  60 0.4}
                        {left 30 0.4}
                        {right 30 0.4}
                        {rear 30 0.2}
		}
	}

	("crew_4_human")

	{boarder
		{anm "left"
			{forward	{begin	"board_tank_left"}}
			{reverse 	{end	"emit_tank_1_hold"}}
		}
		{anm "right"
			{forward	{begin	"board_tank_right"}}
			{reverse 	{end	"emit_tank_1_hold"}}
		}
		{anm "armor"
			{forward	{begin	"board_MiddleArmor"} 	{end "seat_armor"}}
			{reverse 	{end	"emit_tank_1"}}
		}
		{anm "armor1"
			{forward	{begin	"board_MiddleArmor"} 	{end "seat_armor"}}
			{reverse 	{end	"emit_body"}}
		}
	}

	("seat_human" place(seat1) bone(seat01) door(emit3)  animation(armor1))
	("seat_human" place(seat2) bone(seat02) door(emit4)  animation(armor))
	("seat_human" place(seat3) bone(seat03) door(emit5)  animation(armor))
	("seat_human" place(seat4) bone(seat04) door(emit6)  animation(armor1))

	{Weaponry
		{place "gun"
			{DefaultWeapon "75mm_type5"}
			{Mounted}
			{weapon {ammo "ammo bullet75 ap" 1}}
		}
		{place "mgun"
			{DefaultWeapon "type99"}
			{Mounted}
			{weapon {ammo "ammo mgun" 30}}
		}
		{place "mgun2"
			{LinkBone "mgun2"}
			{LinkAnimation "on"}
			{DefaultWeapon "type97_2"}
			{Mounted}
			{weapon {ammo "ammo mgun" 20}}
		}

	}
	{extender "inventory"
		{weapon "gun" {fixed}}
		{box
			{item "ammo mgun" 800}
			{item "ammo bullet75 ap" 39}
			{item "ammo bullet75 fg" 26}
		}
	}
	
	{mass 30500}
	{Chassis
		{track_size 0.6 0.7}
		{Locomotion 
			{MaxSpeed 	    32} ; максимальная скорость, kmph
			{StartTime 		 7} ; время разгона до макс. скорости с 0, seconds
			{BrakeTime 		1} ; время торможения с макс. скорости до 0, seconds
			{StopTime		 1} ; время остановки после отключения двигателя с макс. скорости до 0, seconds
			{TurnRadius     5} ; мин. радиус разворота, meter
			{TurnTime		5} ; время на разворот на 360 градусов при макс. скорости разворота, seconds
			{TurnStart		 2}	; время разгона до макс. угловой скорости с 0, seconds
			{TurnStop	   0.5}	; время остановки с макс. угловой скорости до 0, seconds
			{Gears 0.2 0.3 0.5 0.7 1.0} ; передаточные коэф-ты коробки передач
			{RearGears 0.2 0.5} ; передаточные коэф-ты коробки передач
		}
	}
	{Extension "chi-to.mdl"}


(mod "mp"
	{sensor
		{Visor "main"
			{vision "Visor_Tank_Long"}
		}
	}
	{Chassis
		{speed
			{Slow	9}
			{Normal	14}
			{Fast	32}
		}
		{FuelBag
			{volume 470 }	; liters
			{rate    180 }	; liters per kilometer
			{remain 470 }
			{fuel "fuel"}
		}
	}
)
}