{entity
	(include "/properties/tank.ext")
	(include "/properties/structure/tank_m4.ext")
	(include "/properties/shadow/tank.inc"  scale(1.2))
	(include "/properties/selection/vehicle.inc"  scale(1.2))
	{props "medium"}
	{Volume "Trackright"
	}
	{Volume "Trackleft"
	}
	{Volume "gun"
	}
	{Volume "Crew"
	}
	{Volume "Inventory"
	}
	{Volume "Engine"
	}
	{volume "body1"
		{component "body"}
		{tags "body"}
		{thickness 25
			{front  55}
		}   
	}
	{volume "body2"
		{component "body"}
		{tags "body"}
		{thickness 25
			{front  55}
		}
	}
	{volume "turret"
		{thickness 25 0.3
			{front  60}
		}
	}

	("crew_4_human")	
	("seat_4_human")

	{boarder
		{anm "left"
			{forward	{begin	"board_tank_left"} {end "seat_driver_car"}}
			{reverse 	{end	"emit_tank_1"}}
		}
		{anm "right"
			{forward	{begin	"board_tank_right"} {end "seat_driver_car"}}
			{reverse 	{end	"emit_tank_1"}}
		}
		{anm "armor"
			{forward	{begin	"board_SdKfz251-1"} {end "seat_armor"}}
			{reverse 	{end	"emit_tank_2_hold"}}
		}
	}

	("crew_human_visible"	place(gunner)		door1(emit1) door2(emit2) visor(main))
	("crew_human_visible"	place(commander)	door1(emit1) door2(emit2) visor(main))
	("crew_human"			place(driver)		door1(emit1) door2(emit2) visor(driver))
	("crew_human"			place(charger)		door1(emit1) door2(emit2) visor(main))

	{Weaponry
		{place "gun"
			{DefaultWeapon "90mm_m3"}
			{Mounted}
		}
		{place "mgun"
			{DefaultWeapon "browning"}
			{Mounted}
		}
	}
	{extender "inventory"
		{weapon "gun" {fixed}}
		{box
			{item  "ammo mgun" 650}
			{item "ammo bullet90 ap" 50}
			{item "ammo bullet90 fg" 27}
		}
	}
	
	{mass 30500}
	{Chassis
		{track_size 0.6 0.7}
		{Locomotion ("locomotion")
			{MaxSpeed 	    20} ; максимальная скорость, kmph
			{StartTime 		 6} ; время разгона до макс. скорости с 0, seconds
			{BrakeTime 		0.6} ; время торможения с макс. скорости до 0, seconds
			{StopTime		 1} ; время остановки после отключения двигателя с макс. скорости до 0, seconds
			{TurnRadius     5} ; мин. радиус разворота, meter
			{TurnTime		5} ; время на разворот на 360 градусов при макс. скорости разворота, seconds
			{TurnStart		 2}	; время разгона до макс. угловой скорости с 0, seconds
			{TurnStop	   0.5}	; время остановки с макс. угловой скорости до 0, seconds
			{Gears 0.2 0.3 0.5 0.7 1.0} ; передаточные коэф-ты коробки передач
			{RearGears 0.2 0.5} ; передаточные коэф-ты коробки передач
		}
	}
	{Extension "M10achilles.mdl"}
}
