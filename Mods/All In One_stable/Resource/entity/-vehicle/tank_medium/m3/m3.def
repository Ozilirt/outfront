{entity
	(include "/properties/tank.ext")
	(include "/properties/structure/tank_m4.ext")
	(include "/properties/shadow/tank.inc"  scale(1.2))
	(include "/properties/selection/vehicle.inc"  scale(1.2))
	{props "medium" "turret_rotator" "high"}
	
	
	{volume "body1"
		{component "body"}
		{tags "body"}
		{thickness 38
			{front 38}
                  {top	20}
			{bottom	20}
		}   
	}
	{volume "body2"
		{component "body"}
		{tags "body"}
		{thickness 38
			{front  38}
                  {top	20}
			{bottom	20}
		}
