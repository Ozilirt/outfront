{entity
	(include "/properties/tank_unarmed.ext")
	(include "/properties/shadow/tank.inc"  scale(1.2))
	(include "/properties/selection/vehicle.inc"  scale(1.2))
	{props "heavy" "mine_exploder" "low"}

	{TexMods
		{"ambush" "1"}
		{"ambush2" "2"}
		{"ambush3" "3"}
		{"ambush4" "4"}
		{"ambush5" "5"}
		{"ambush6" "6"}
		{"ambush7" "7"}
		{"ambush8" "8"}
	}
	{volume "body1"
		{component "body"}
		{tags "body"}
		{thickness 30
			{front  80}
			{bottom	30}
		}   
	}
	{volume "body2"
		{component "body"}
		{tags "body"}
		{thickness 30
			{front  80}
			{bottom	30}
		}
	}
	{volume "support1"
		{thickness 50}
	}
	{volume "support"
