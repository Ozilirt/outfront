{entity
	(include "/properties/tank.ext")
	(include "/properties/structure/tank_t34.ext")
	(include "/properties/shadow/tank.inc"  scale(1.3))
	(include "/properties/selection/vehicle.inc"  scale(1.2))
	{props "medium" "turret_rotator"}
	
	{TexMods
		{"ambush1" "1"}
		{"ambush2" "2"}
	}
	
	{volume "body1"
		{component "body"}
		{tags "body"}
		{thickness 45
			{top	20}
			{bottom	20}
		}   
	}
	{volume "body2"
		{component "body"}
		{tags "body"}
		{thickness 45
			{top	20}
			{bottom	20}
		}
	}
	{volume "turret"
		{thickness 45 0.3
			{front  52}
			{rear   20}
			{top    20}
		}
	}

	("crew_4_human")

	{boarder
		{anm "left"
			{forward	{begin	"board_t-34"}}
			{reverse 	{end	"emit_tank_1_hold"}}
		}
		{anm "right"
			{forward	{begin	"board_t-34"}}
			{reverse 	{end	"emit_tank_1_hold"}}
		}
		{anm "armor"
			{forward	{begin	"board_HighArmor"} 	{end "seat_armor"}}
			{reverse 	{end	"emit_tank_2"}}
		}
		{anm "armor1"
			{forward	{begin	"board_HighArmor"} 	{end "seat_armor"}}
			{reverse 	{end	"emit_body"}}
		}
		{anm "armor2"
			{forward	{begin	"board_MiddleArmor"} {end "seat_armor"}}
			{reverse 	{end	"emit_tank_2"}}
		}
	}

	("seat_human" place(seat1) bone(seat01) door(emit3)  animation(armor1))
	("seat_human" place(seat2) bone(seat02) door(emit4)  animation(armor))
	("seat_human" place(seat3) bone(seat03) door(emit5)  animation(armor1))
	("seat_human" place(seat4) bone(seat04) door(emit6)  animation(armor2))

	{Weaponry
		{place "gun"
			{DefaultWeapon "76mm_L11"}
			{Mounted}
		}
		{place "mgun"
			{DefaultWeapon "dt"}
			{Mounted}
			{weapon {ammo "ammo mgun" 60}}
		}
	}
	{extender "inventory"
		{weapon "gun" {fixed}}
		{box
			{item  "ammo mgun" 500}
			{item "ammo bullet76 ap" 50}
			{item "ammo bullet76 fg" 27}
		}
	}
	
	{mass 26500}
	{Chassis
		{track_size 0.6 0.7}
		{Locomotion ("locomotion")
			{MaxSpeed 	    18} ; максимальная скорость, kmph
			{StartTime 		 6} ; время разгона до макс. скорости с 0, seconds
			{BrakeTime 		0.6} ; время торможения с макс. скорости до 0, seconds
			{StopTime		 1} ; время остановки после отключения двигателя с макс. скорости до 0, seconds
			{TurnRadius     5} ; мин. радиус разворота, meter
			{TurnTime		5} ; время на разворот на 360 градусов при макс. скорости разворота, seconds
			{TurnStart		 2}	; время разгона до макс. угловой скорости с 0, seconds
			{TurnStop	   0.5}	; время остановки с макс. угловой скорости до 0, seconds
			{Gears 0.2 0.3 0.5 0.7 1.0} ; передаточные коэф-ты коробки передач
			{RearGears 0.2 0.5} ; передаточные коэф-ты коробки передач
		}
	}
	{extension "T-34-76-41.mdl"}
	(mod "mp"
		{sensor
			{Visor "main"
				{vision "Visor_Tank"}
			}
		}
		{Chassis
			{Locomotion ("locomotion")
				{maxspeed 24}
			}
			{speed
				{Slow	12	}
				{Normal	18}
				{Fast	24	}
			}
			{FuelBag
				{volume 560 } ; liters
				{rate   200 } ; liters per kilometer
				{remain 420 }
				{fuel "fuel"}
			}
		}
	)
}
